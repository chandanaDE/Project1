{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ProjectDemoDataFactory"
		},
		"Destdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Destdatalake'"
		},
		"Linked_blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Linked_blobstorage'"
		},
		"outputpipeline2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'outputpipeline2'"
		},
		"salesdestdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'salesdestdb'"
		},
		"Destdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destdatalakestorage.dfs.core.windows.net/"
		},
		"outputpipeline2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destdatalakestorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ProjectDemoDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Destdatalake_accountKey": {
						"value": "[parameters('Destdatalake_accountKey')]"
					},
					"Linked_blobstorage_connectionString": {
						"value": "[parameters('Linked_blobstorage_connectionString')]"
					},
					"outputpipeline2_accountKey": {
						"value": "[parameters('outputpipeline2_accountKey')]"
					},
					"salesdestdb_connectionString": {
						"value": "[parameters('salesdestdb_connectionString')]"
					},
					"Destdatalake_properties_typeProperties_url": {
						"value": "[parameters('Destdatalake_properties_typeProperties_url')]"
					},
					"outputpipeline2_properties_typeProperties_url": {
						"value": "[parameters('outputpipeline2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}